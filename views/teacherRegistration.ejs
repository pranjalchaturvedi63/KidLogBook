                            </div>
                            <div class="form-group">
                                <input type="text" class="form-control border-0 p-1"
                                    placeholder="Enter 10 Digits Contact Number" name="contact" id="contact"
                                    required="required" />
                            </div>

                            <div class="form-group">  
                                <select class="form-control border-0 p-1" name="qualification" id="qualification"
                                    required>
                                    <option value="">Select Qualification</option>
                                    <option value="BBA">BBA</option>
                                    <option value="BCA">BCA</option>
                                    <option value="BSC">BSC</option>
                                    <option value="BCOM">BCOM</option>
                                    <option value="MBA">MBA</option>
                                    <option value="MCA">MCA</option>
                                    <option value="MSC">MSC</option>
                                    <option value="MCOM">MCOM</option>
                                    <option value="BTech">BTech</option>
                                    <option value="MTech">MTech</option>
                                </select>

                            </div>
                            <div class="form-group">
                                <select class="form-control border-0 p-1" name="experience" id="experience" required>
                                    <option value="">Select Experience</option>
                                    <option value="Fresher">Fresher</option>
                                    <option value="6+ Months Experienced">6+ Months Experienced</option>
                                    <option value="1+ Year Experience">1+ Year Experience</option>
                                    <option value="3+ Year Experience">3+ Year Experience</option>
                                    <option value="5+ Year Experience">5+ Year Experience</option>
                                </select>

                            </div>

                        </div>
                        <div class="col-lg-6 card-body rounded-bottom bg-primary p-4">
                            <span class="text-secondary">Select Date Of Birth : </span>

                            <div class="form-group">
                                <input type="date" class="form-control border-0 p-1" name="date" id="date"
                                    required="required" />
                            </div>


                            <div class="form-group">
                                <span class="text-secondary">Select Gender : </span>
                                <input type="radio" class="radio-inline" name="gender" id="male" value="Male"
                                    required="required" />
                                <span class="text-white">Male</span>
                                <input type="radio" class="radio-inline" name="gender" id="female" value="Female"
                                    required="required" /> <span class="text-white">Female</span>

                            </div>
                            <div class="form-group">
                                <textarea class="form-control border-0 p-1" placeholder="Enter Address" name="address"
                                    id="address" required="required"></textarea>
                            </div>

                            <div>
                                <button class="btn btn-secondary btn-block border-0 py-2"
                                    type="submit">Register</button>
                            </div>
                            <br>
                            <div>
                                <button class="btn btn-secondary btn-block border-0 py-2" type="reset">Reset</button>
                            </div>

